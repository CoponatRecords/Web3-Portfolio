{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "resolveJsonModule": true 

  },
  "include": [
    "src/**/*",
    "node_modules/@mui/icons-material/**/*",
    "components/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
